{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/about",
      "rewrite": "/about.html"
    },
    {
      "route": "/features", 
      "rewrite": "/features.html"
    },
    {
      "route": "/contact",
      "rewrite": "/contact.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/css/*", "/js/*", "/screens/*", "/logo/*", "/assets/*"]
  },
  "mimeTypes": {
    ".png": "image/png",
    ".gif": "image/gif",
    ".css": "text/css",
    ".js": "application/javascript",
    ".html": "text/html"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
